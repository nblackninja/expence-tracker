  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-validator": "^6.12.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.6",
    "morgan": "^1.10.0",
